{
  "cSpell.language": "en,de",
  "editor.insertSpaces": true,
  "editor.rulers": [80],
  "editor.formatOnSave": true,
  "[python]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "liveServer.settings.port": 5502,
  "cSpell.words": [
    "alph",
    "batchmode",
    "cellaligns",
    "darray",
    "Divs",
    "documentclass",
    "ECTS",
    "emummy",
    "eqnarray",
    "floatright",
    "FOSS",
    "glqq",
    "grqq",
    "includegraphics",
    "infty",
    "Kapitelauswahl",
    "katex",
    "lbrack",
    "ldquo",
    "LREF",
    "mathbb",
    "mtable",
    "nodenext",
    "outfile",
    "pdflatex",
    "randint",
    "rdquo",
    "rwth",
    "showhide",
    "tableofcontents",
    "texlive",
    "textbf",
    "textit",
    "tikz",
    "usepackage",
    "usetikzlibrary",
    "visualizationwrapper"
  ],
  "latex-workshop.latex.autoBuild.run": "never",
  "[tex]": {
    "editor.formatOnSave": false
  }
}
