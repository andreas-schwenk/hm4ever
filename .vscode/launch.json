{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test index-dev.html",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5502/index-dev.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder"
      }
    }
  ]
}
